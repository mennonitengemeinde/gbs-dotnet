@inherits BaseCascadingState<StudentResponse, CreateStudentRequest, int, CreateStudentRequest>

<CascadingValue Value="this">
    @ChildContent
</CascadingValue>

@code {
    public override string BaseUrl => "api/students";

    public async Task<StudentResponse?> GetById(int id)
    {
        await Fetch();
        return Data.FirstOrDefault(c => c.Id == id);
    }

}