@inherits BaseCascadingState<GenerationResponse, CreateGenerationRequest, int, UpdateGenerationRequest>

<CascadingValue Value="this">
    @ChildContent
</CascadingValue>

@code {
    public override string BaseUrl => "api/generations";
    
    public async Task<GenerationResponse?> GetById(int id)
    {
        await Fetch();
        return Data.FirstOrDefault(c => c.Id == id);
    }
}